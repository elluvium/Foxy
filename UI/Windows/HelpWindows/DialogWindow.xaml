    <Grid>
        <TextBlock x:Name="textBlockMessage" Margin="10,10,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="151"/>
        <Button x:Name="buttonOK" Content="OK" HorizontalAlignment="Left" Margin="251,187,0,0" VerticalAlignment="Top" Height="20" FontSize="12" Width="75" Click="buttonOK_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="351,187,0,0" VerticalAlignment="Top" Height="20" FontSize="12" Width="75" Click="buttonCancel_Click"/>

    </Grid>
</Window>
